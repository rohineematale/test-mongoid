    .sb-slidebar.sb-right.sb-style-overlay
      %h5.side-title Online Customers
      %ul.quick-chat-list
        %li.online
          .media
            %a.pull-left.media-thumb{:href => "#"}
              %img.media-object{:alt => "", :src => "img/chat-avatar2.jpg"}
            .media-body
              %strong John Doe
              %small Dream Land, AU
          / media
        %li.online
          .media
            %a.pull-left.media-thumb{:href => "#"}
              %img.media-object{:alt => "", :src => "img/chat-avatar.jpg"}
